<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.superfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Regular_user_leaderboard"></activity>
        <activity android:name=".Regular_user_profile" />
        <activity android:name=".Regular_user_donate" />
        <activity android:name=".Regular_user_volunteer" />
        <activity
            android:name=".Regular_user_home_screen"
            android:label="@string/title_activity_regular_user_home_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EmployeeLogin" />
        <activity android:name=".SignUp" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>